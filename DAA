[02/11, 12:26] ʀ ɢ ᴘʀᴀꜱᴀɴɴᴀ: #include <iostream>
#include <climits>
using namespace std;

int main() {
    int n;
    cin >> n;

    int cost[20][20];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> cost[i][j];

    int selected[20] = {0};
    selected[0] = 1;  // Start from city 1 (index 0)
    int edges = 0;

    while (edges < n - 1) {
        int minCost = INT_MAX;
        int x = 0, y = 0;

        for (int i = 0; i < n; i++) {
            if (selected[i]) {
                for (int j = 0; j < n; j++) {
                    if (!selected[j] && cost[i][j] != 0 && cost[i][j] != 9999) {
                        if (cost[i][j] < minCost) {
                            minCost = cost[i][j];
                            x = i;
                            y = j;
                        }
                    }
                }
            }
        }

        cout << x + 1 << " -> " << y + 1 << endl;
        selected[y] = 1;
        edges++;
    }

    return 0;
}
